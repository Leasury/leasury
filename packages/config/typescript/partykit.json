{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "PartyKit",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020", "DOM"],
        "module": "ES2020",
        "noEmit": true,
        "jsx": "react-jsx",
        "verbatimModuleSyntax": true,
        "resolveJsonModule": true
    }
}
